{
    "name": "routeros-syntax",
    "displayName": "RouterOS Syntax Highlighting",
    "description": "Syntax Highlighting for MikroTik RouterOS scripts",
    "version": "1.0.0",
    "publisher": "cperezabo",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "routeros",
            "aliases": ["RouterOS", "routeros"],
            "extensions": [".rsc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "routeros",
            "scopeName": "source.routeros",
            "path": "./syntaxes/RouterOS.tmLanguage"
        }]
    }, 
    "icon": "images/icon.png",
    "license": "MIT"
}